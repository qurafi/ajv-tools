{
    "name": "ajv-build-tools",
    "description": "",
    "version": "0.0.1",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./unplugin": {
            "types": "./dist/unplugin/index.d.ts",
            "require": "./dist/unplugin/index.js",
            "import": "./dist/unplugin/index.mjs"
        },
        "./core": {
            "types": "./dist/core/index.d.ts",
            "require": "./dist/core/index.js",
            "import": "./dist/core/index.mjs"
        },
        "./types": {
            "types": "./types/index.d.ts"
        }
    },
    "files": [
        "dist",
        "*.d.ts",
        "types/*.d.ts",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "test": "DEBUG_COLORS=1 vitest",
        "build": "tsup 'src/**/index.ts' --format esm,cjs --dts --sourcemap",
        "format": "prettier -w src/**/*.ts",
        "dev": "npm run build -- --watch",
        "ts:check": "tsc --noEmit",
        "pkglint": "publint"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "ajv": "^8.12.0",
        "ajv-errors": "^3.0.0",
        "ajv-formats": "^2.1.1",
        "chokidar": "^3.5.3",
        "debug": "^4.3.4",
        "esbuild": "^0.17.14",
        "fast-glob": "^3.2.12",
        "fs-extra": "^11.1.1",
        "kleur": "^4.1.5",
        "micromatch": "^4.0.5",
        "rfdc": "^1.3.0",
        "unplugin": "^1.3.1"
    },
    "devDependencies": {
        "@types/debug": "^4.1.7",
        "@types/fs-extra": "^11.0.1",
        "@types/micromatch": "^4.0.2",
        "@types/node": "^18.15.6",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "fs-extra": "^11.1.1",
        "prettier": "^2.8.7",
        "publint": "^0.1.11",
        "puppeteer": "^20.2.0",
        "tsup": "^6.7.0",
        "typescript": "^5.0.2",
        "vite": "^4.3.2",
        "vitest": "^0.29.7"
    }
}
