{
    "name": "ajv-build-tools",
    "private": true,
    "description": "",
    "version": "0.0.1",
    "main": "./dist/main.js",
    "module": "./dist/main.mjs",
    "types": "./dist/main.d.ts",
    "exports": {
        "./unplugin": {
            "types": "./dist/unplugin/main.d.ts",
            "require": "./dist/unplugin/main.js",
            "import": "./dist/unplugin/main.mjs"
        },
        "./core": {
            "types": "./dist/core/main.d.ts",
            "require": "./dist/core/main.js",
            "import": "./dist/core/main.mjs"
        }
    },
    "scripts": {
        "test": "vitest",
        "build": "tsup 'src/*/main.ts' --format esm,cjs --dts --sourcemap",
        "dev": "npm run build -- --watch",
        "ts:check": "tsc --noEmit",
        "pkglint": "publint"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "unplugin": "^1.3.1",
        "ajv": "^8.12.0",
        "debug": "^4.3.4",
        "esbuild": "^0.17.14",
        "fast-glob": "^3.2.12",
        "fs-extra": "^11.1.1",
        "kleur": "^4.1.5",
        "micromatch": "^4.0.5",
        "ajv-formats": "^2.1.1"
    },
    "devDependencies": {
        "@types/node": "^18.15.6",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@types/debug": "^4.1.7",
        "@types/fs-extra": "^11.0.1",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "prettier": "^2.8.7",
        "publint": "^0.1.11",
        "tsup": "^6.7.0",
        "typescript": "^5.0.2",
        "vitest": "^0.29.7",
        "fs-extra": "^11.1.1",
        "vite": "^4.2.1",
        "@types/micromatch": "^4.0.2"
    }
}
